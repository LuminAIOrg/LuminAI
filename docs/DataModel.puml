@startuml
'to group stuff
class group{
 id: number;
 name: string;
}

class sensor{
 id: number;
 name: string;
 device_id: number; FK
 type_id: number; FK
 unit: string;
}

class sensor_data{
 id: number;
 sensor_id: number; FK
 value: number;
 timestamp: number;
}

'producer or consumer...
class type{
 id: number
 name: string
}


group "1" -- "*" sensor
sensor "1" -- "*" sensor_data
type "1" -- "*" sensor
@enduml